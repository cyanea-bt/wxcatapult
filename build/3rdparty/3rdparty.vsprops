<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="3rdparty"
	>
	<UserMacro
		Name="DerivedDir"
		Value="..\..\derived"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="3rdPartySrcDir"
		Value="$(DerivedDir)\3rdparty\src"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="BuildFlavor"
		Value="$(PlatformName)-VC-$(ConfigurationName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="BuildDir"
		Value="$(DerivedDir)\$(BuildFlavor)"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="3rdPartyBuildDir"
		Value="$(BuildDir)\3rdparty"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="3rdPartyIntDir"
		Value="$(3rdPartyBuildDir)\build"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="3rdPartyOutDir"
		Value="$(3rdPartyBuildDir)\install\lib"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="CatapultIntDir"
		Value="$(BuildDir)\build"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="CatapultOutDir"
		Value="$(BuildDir)\install"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="CatapultConfigDir"
		Value="$(BuildDir)\config"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="LibNameLibXml"
		Value="libxml2-2.7.2"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="LibNameWx"
		Value="wxMSW-2.8.9"
		PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>
